<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="carros_xamarin_clean.Features.Car.Presentation.Pages.CarView"
             xmlns:pages="clr-namespace:carros_xamarin_clean.Features.Car.Presentation.Pages"
             NavigationPage.HasNavigationBar="False">
  <FlyoutPage.Flyout>
    <pages:CarViewFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:CarListView />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>